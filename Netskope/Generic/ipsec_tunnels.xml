  <command name="CREATE">
    <operation><![CDATA[{literal}{{/literal}
"enable": {$params.enable|default:'false'},

"pops": [
  {foreach $params.pops as $pop}
    "{$ipsec_pops.{$pop.pop_name}.variable_name|default:''}"{if !$pop@last}, {/if}
  {/foreach}
],

"site": "{$params.site|default:'site1'}",
"sourcetype": "{$params.sourcetype|default:'User'}",
"srcidentity": "{$params.srcidentity|default:'1.1.1.1'}",

"srcipidentity": {if $params.srcipidentity != ''}"{$params.srcipidentity}"{else}null{/if},

"psk": "{$params.psk|default:'bhEiMx2fhfHitBVvdxoxx27qrMj4HwLV'}",
"encryption": "{$params.encryption|default:'AES128-CBC'}",

{if isset($params.vendor)}
"vendor": "{$params.vendor|default:'Default'}",
{/if}

"bandwidth": {$params.bandwidth|default:50},

"options": {
  "xff": {
    "enable": {if $params.xff_enable == true}true{else}false{/if}
    {if $params.xff_enable == true && isset($params.xff)},
    "iplist": [
      {foreach $params.xff as $ip_list}
        "{$ip_list.ip|default:''}"{if !$ip_list@last}, {/if}
      {/foreach}
    ]
    {/if}
  }{if $params.rekey == true || $params.reauth == true},{/if}
  {if $params.rekey == true}
  "rekey": true{if $params.reauth == true}, {/if}
  {/if}
  {if $params.reauth == true}
  "reauth": true
  {/if}
}
{literal}}{/literal}]]></operation>
    <rest>POST</rest>
    <xpath>/api/v2/steering/ipsec/tunnels</xpath>
  </command>
